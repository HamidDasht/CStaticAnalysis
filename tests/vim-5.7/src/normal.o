  break;
 case 'W':
  flag = current_word(cap->oap, cap->count1, include, 1);
  break;
 case 'b':
 case '(':
 case ')':
  flag = current_block(cap->oap, cap->count1, include, '(', ')');
  break;
 case 'B':
